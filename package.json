{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "author": "Ashiqur Rahman - iamashiqur",
  "description": "iamashiqur.vercel.app",
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": "npm run lint-prettier"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc --noEmit --pretty",
    "lint": "next lint",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.jsx,.ts,.tsx .",
    "prettier:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|jsx|ts|tsx|json)\"",
    "prettier:fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "lint-prettier": "npm run lint:check && npm run prettier:fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@eslint/config-array": "^0.19.0",
    "@eslint/object-schema": "^2.1.4",
    "@phosphor-icons/react": "^2.1.7",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.5",
    "axios-retry": "^4.5.0",
    "glob": "^11.0.0",
    "lint-staged": "^15.2.10",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.2",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
